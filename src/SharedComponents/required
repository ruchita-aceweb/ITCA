    <div className={`w-full lg:w-1/4 p-4 bg-about text-center  lg:m-0 mb-2  ${isOpen26 ? 'h-auto' : 'h-24'}`}>
      <h1 className="text-xl cursor-pointer mt-2" onClick={toggleAccordion26}>
      News
      </h1>
      {isOpen26 && (
         <div>
         <p className="mt-2" data-aos="fade-down">
           "Coming Soon "
         </p>
         
         </div>
      )}
    </div>
    
  
  
  
  </div>





  
    </>
  );
};

export default Home;
